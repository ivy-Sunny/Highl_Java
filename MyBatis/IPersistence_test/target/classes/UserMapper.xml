<mapper namespace="user">
    <select id="selectList" resultType="com.ivy.pojo.User">
        select * from user;
    </select>
    <select id="selectOne" resultType="com.ivy.pojo.User" paramterType="com.ivy.pojo.User">
        select * from user where id = #{id} and username = #{username};
    </select>
</mapper>